            <div class="datos, cajitaDato">
              <label class="control-label" for="derechos">Derechos de autor*</label>
              <input name="derechos"type="text" placeholder="Derechos de autor"  value="<%=producto.derechosAutor%>" />
              <div class="messages"></div>
            </div>
              
            <div class="datos, cajitaDato">
              <label class="control-label" for="autor">Seleccione una opción</label>
              <select name="autor"   value="<%=producto.asociado%>">
